{
  "manifest_version": 3,
  "name": "Hyprice",
  "version": "1.0",
  "description": "The ultimate extension for Hyperliquid price tracking and personalized token watchlists.",
  "permissions": ["storage", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
